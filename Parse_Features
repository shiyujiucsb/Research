#include <string>
#include <iostream>
#include <sstream>
#include <fstream>

using namespace std;

bool contains_digits_colon_period(const std::string &str) {
    return str.find_first_not_of("0123456789:.") == std::string::npos;
  }
bool all_digits(const std::string &str){
    return str.find_first_not_of("0123456789") == std::string::npos;
}

int main()
{
  ifstream infile("file1.txt");
  string line;
  string token_array[10000];
  string delimiter1 = " ";
  string delimiter2 = ":";
 
 
  int counter = 0;
  size_t pos = 0;
  size_t token_pos = 0;
  for( std::string line; getline( infile, line ); )
  {
    while ((pos = line.find(delimiter1)) != std::string::npos) 
    {
      string token = line.substr(0, pos);
      if((contains_digits_colon_period(token)) && !(all_digits(token)) )
      {
        token_array[counter] = token;
        cout << token << endl;
        counter+=1;
      }
      line.erase(0, pos + delimiter1.length());
    }
    cout<<"printing token vals"<<endl;
    for(string token : token_array){
        token_pos = token.find(delimiter2);
        token.erase(0, token_pos + delimiter2.length());
        cout<<token<<endl;
    }
    break;
    
    
  }
  return 0;
}
